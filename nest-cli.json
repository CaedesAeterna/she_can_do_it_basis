{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/food-waste-customer/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/food-waste-customer/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/food-waste-customer",
  "projects": {
    "food-waste-cms": {
      "type": "application",
      "root": "apps/food-waste-cms",
      "entryFile": "main",
      "sourceRoot": "apps/food-waste-cms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/food-waste-cms/tsconfig.app.json",
        "plugins": ["@nestjs/swagger"]
      }
    },
    "food-waste-customer": {
      "type": "application",
      "root": "apps/food-waste-customer",
      "entryFile": "main",
      "sourceRoot": "apps/food-waste-customer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/food-waste-customer/tsconfig.app.json",
        "plugins": ["@nestjs/swagger"]
      }
    }
  }
}